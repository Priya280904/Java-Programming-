import java.util.*;
class Main {
    public static void main(String[] args) {
        String  A = "New Year new world";
        String  P = "[ ,\\ .//s]";
        String [] z= A.split(P);
        for(String s:z){
       System.out.println(s);
            
        }
        
    }
}
//output:
New
Year
new
world
