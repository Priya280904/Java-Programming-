import java.util.Scanner;
class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String x ="PriyaArun";
        String sub=x.substring(0,5);
        String sub1=x.substring(5);
        System.out.println(sub+" "+sub1);
    }
}
