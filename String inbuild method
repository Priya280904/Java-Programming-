import java.util.*;
class Main {
    public static void main(String[] args) {
        String  A = "New world";
        String  P = "Hello world";
       System.out.println(A.replace('e', 'p'));
        String a ="(?!)Hello";
        System.out.println(P.replaceAll(a,"priya"));
        String b = "Hello";
        System.out.println(P.replaceFirst(b,"priya"));
    }
}
//output:
Npw world
Hello world
priya world

