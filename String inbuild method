import java.util.*;
class Main {
    public static void main(String[] args) {
        String  A = "New world";
        String  P = "Hello world";
        String x = "priya|world|java";
        System.out.println("Hello".matches(x));
        System.out.println("world".matches(x));
        System.out.println("java".matches(x));
        System.out.println(P.regionMatches(6,A,4,5));
    }
}
//output:
false
true
true
true
